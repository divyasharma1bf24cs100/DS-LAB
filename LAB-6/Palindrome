/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     struct ListNode *next;
 * };
 */
bool isPalindrome(struct ListNode* head) {
    struct ListNode *temp=head;
    int arr[100000];
    int n=0;
    while(temp!=NULL){
        arr[n++]=temp->val;
        temp=temp->next;
    }
    int i=0, j=n-1;
    while(i<j){
        if(arr[i]!=arr[j]){
            return false;
        }
        i++;
        j--;
    }
    return true;
}
